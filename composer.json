{
  "name": "treelink/helper",
  "type": "library",
  "description": "PHP 开发时常用的工具",
  "version": "1.2.2",
  "license": "MIT",
  "authors": [
    {
      "name": "trink",
      "email": "treelink95@gmail.com",
      "homepage": "https://github.com/easygoto"
    }
